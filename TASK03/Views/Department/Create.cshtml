@model Department

<h2>Add New Department</h2>
<br />
<hr />
<br />

<form action="~/Department/Create" method="post">

    <div class="form-group">
        <label for="Name" class="form-label">Department Name</label>
        <input type="text" id="Name" name="DeptName" class="form-control" />
    </div>

    <input type="submit" value="Create Department" class="btn btn-success my-5" />
    <a href="~/Department/Index" class="btn btn-secondary">Cancel</a>

</form>